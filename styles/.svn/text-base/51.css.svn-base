@keyframes moveright{
	0%{
		transform:rotate(-3deg) ;
	}
	2%{
		transform:rotate(-5deg) ;
	}
	4%{
		transform:rotate(-7deg) ;
	}
	6%{
		transform:rotate(-9deg) ;
	}
	8%{
		transform:rotate(-11deg) ;
	}
	10%{
		transform:rotate(-13deg) ;
	}
	12%{
		transform:rotate(-16deg) ;
	}
	
}

@-webkit-keyframes moveright{
	0%{
		transform:rotate(-3deg) ;
	}
	2%{
