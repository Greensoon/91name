@keyframes eyeclose1 {
	0% {
		top: 19px;
	}
	10% {
		top: 28px;
	}
	20% {
		top: 19px;
	}
	20% {
		top: 28px;
	}
	40% {
		top: 19px;
	}
}
@-moz-keyframes eyeclose1
/* Firefox */

{
	0% {
		top: 19px;
	}
	10% {
		top: 28px;
	}
	20% {
		top: 19px;
	}
	20% {
		top: 27px;
	}
	40% {
		top: 19px;
	}
}
@-webkit-keyframes eyeclose1
/* Safari 和 Chrome */

{
	0% {
		top: 19px;
	}
	5% {
		top: 27px;
	}
	10% {
		top: 19px;
	}
	15% {
		top: 27px;
	}
	20% {
		top: 19px;
	}
}
.n-cover-s {
	animation: eyeclose1 3s infinite 2s;
	-moz-animation: eyeclose1 3s infinite 2s;
	/* Firefox */
	
	-webkit-animation: eyeclose1 3s infinite 2s;
	/* Safari 和 Chrome */
}
